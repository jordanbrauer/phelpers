{
    "name": "jordanbrauer/phelpers",
    "description": "Collection of random helper QoLfunctions for PHP.",
    "authors": [
        {
            "name": "Jordan Brauer",
            "email": "18744334+jordanbrauer@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phelpers\\": "src/"
        },
        "files": [
            "src/append.php",
            "src/blank.php",
            "src/camel_case.php",
            "src/head.php",
            "src/kebab_case.php",
            "src/only.php",
            "src/ordinal.php",
            "src/pascal_case.php",
            "src/retry.php",
            "src/snake_case.php",
            "src/swap.php",
            "src/tail.php",
            "src/tap.php",
            "src/transform.php",
            "src/value.php",
            "src/with.php",
            "src/wrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Phelpers\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^3.4"
    }
}
